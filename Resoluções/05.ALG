algoritmo "cinco"
var
   n1, n2, med : real
inicio
      escreva("Digite a primeira nota: ")
      leia(n1)
      escreva("Digite a segunda nota: ")
      leia(n2)
      med <- (n1+n2) / 2
      escreval("Nota 1: ", n1)
      escreval("Nota 2: ", n2)
      escreval("A média entre ", n1," e ", n2," é igual a ", med)
fimalgoritmo